# Copy this file to .env and update the values for your environment.

# Azure AI Foundry project endpoint (e.g., https://<resource>.services.ai.azure.com/api/projects/<project>)
PROJECT_ENDPOINT=

# Azure AI Search service endpoint (e.g., https://<search-name>.search.windows.net)
AZURE_SEARCH_ENDPOINT=

# Logical names for created resources (override to avoid collisions)
AZURE_SEARCH_INDEX=
AZURE_SEARCH_KNOWLEDGE_SOURCE_NAME=
AZURE_SEARCH_AGENT_NAME=

# Azure OpenAI resource configuration
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_GPT_DEPLOYMENT=gpt-4.1-mini
AZURE_OPENAI_GPT_MODEL=gpt-4.1-mini
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-3-large
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-large

# Foundry agent model (defaults to GPT-4.1-mini if not set)
AGENT_MODEL=gpt-4.1-mini
